{
  "name": "meep_bot",
  "version": "1.0.6",
  "description": "Discord Bot",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "start": "npm run build && npm run run",
    "run": "node -r dotenv/config lib/index.js",
    "super": "npm run lint && npm test && npm run build && npm run run",
    "build": "rm -rf lib/* && cp src/assets lib/ -r && tsc --project tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:js": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "tslint -p tsconfig.json --fix",
    "prepublishOnly": "npm run lint && npm run format && npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quentin-correia/MeepBot.git"
  },
  "keywords": [],
  "author": "hubble34",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/quentin-correia/MeepBot/issues"
  },
  "homepage": "https://github.com/quentin-correia/MeepBot#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node-fetch": "^2.5.10",
    "@types/ws": "^7.4.4",
    "@types/xml2js": "^0.4.8",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^27.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@discordjs/opus": "^0.5.2",
    "@discordjs/voice": "^0.3.0",
    "@types/jsdom": "^16.2.11",
    "better-sqlite-pool": "^0.3.1",
    "better-ytdl": "^1.1.1",
    "bufferutil": "^4.0.3",
    "discord.js": "^12.5.3",
    "discord.js-buttons": "^1.0.0",
    "dotenv": "^10.0.0",
    "e621": "^1.0.8",
    "enmap": "^5.8.5",
    "jsdom": "^16.6.0",
    "magmaplayer": "^0.2.2",
    "nhentai.js-api": "^2.2.1",
    "node-fetch": "^2.6.1",
    "simple-spotify": "^1.0.1",
    "soundcloud.ts": "^0.4.0",
    "utf-8-validate": "^5.0.5",
    "xml2js": "^0.4.23",
    "ytdl-core": "^4.8.2",
    "ytpl": "^2.2.1",
    "ytsr": "^3.5.0"
  }
}
