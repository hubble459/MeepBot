{
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "rules": {
        "no-empty": [
            true,
            "allow-empty-catch",
            "allow-empty-functions"
        ],
        "indent": [
            true,
            "spaces",
            4
        ],
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "no-console": false,
        "no-unused-expression": false
    }
}